{
  "name": "blinkinstall",
  "productName": "blinkinstall",
  "version": "2.0.0",
  "description": "The desktop installer for blinkOS, v2.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [
    "OS",
    "Installer",
    "Mobile Phone",
    "Phone",
    "BlinkOS",
    "hackSugar"
  ],
  "author": {
    "name": "Connor",
    "email": "cam-o-man@live.com"
  },
  "license": "GNU General Public License v3.0",
  "config": {
    "forge": {
      "packagerConfig": {},
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip",
          "dmg"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "blinkinstall"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "format": "ULFO"
          }
        }
      ]
    }
  },
  "dependencies": {
    "@cam-o-man/simpleshell": "1.2.1",
    "curl": "^0.1.4",
    "electron-squirrel-startup": "^1.0.0",
    "events": "^3.0.0",
    "glob": "^7.1.4",
    "interpret": "^1.2.0",
    "node-wget": "^0.4.3",
    "rechoir": "^0.7.0",
    "remote": "^0.2.6"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.43",
    "@electron-forge/maker-deb": "^6.0.0-beta.43",
    "@electron-forge/maker-dmg": "^6.0.0-beta.43",
    "@electron-forge/maker-rpm": "^6.0.0-beta.43",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.43",
    "@electron-forge/maker-zip": "^6.0.0-beta.43",
    "electron": "11.5.0"
  }
}
